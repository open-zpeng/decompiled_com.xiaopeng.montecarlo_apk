<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <stroke android:width="2dp" android:color="@color/primary_positive_normal" android:dashWidth="0dp" android:dashGap="0dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <stroke android:width="2dp" android:color="@color/traffic_event_i_know_text_color" android:dashWidth="0dp" android:dashGap="0dp"/>
        </shape>
    </item>
</selector>
