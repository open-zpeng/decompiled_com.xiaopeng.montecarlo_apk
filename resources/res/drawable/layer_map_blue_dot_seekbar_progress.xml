<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:height="@dimen/volume_panel_progress_height" android:width="@dimen/volume_panel_progress_width" android:left="@dimen/volume_panel_progress_padding" android:top="@dimen/volume_panel_progress_padding" android:right="@dimen/volume_panel_progress_padding" android:bottom="@dimen/volume_panel_progress_padding">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/volume_panel_radius"/>
            <solid android:color="@color/volumn_progress_color"/>
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@+id/progress_low" android:height="@dimen/volume_panel_icon_size" android:width="@dimen/volume_panel_icon_size" android:left="@dimen/volume_panel_icon_level0_left">
        <level-list>
            <item android:drawable="@drawable/vector_ic_mid_volume_low_26_blue" android:maxLevel="10000"/>
            <item android:drawable="@drawable/vector_ic_mid_volume_mute_26_grey" android:minLevel="10000" android:maxLevel="10001"/>
        </level-list>
    </item>
    <item android:gravity="center_vertical" android:height="@dimen/volume_panel_tick0_width" android:width="@dimen/volume_panel_tick_height" android:drawable="@drawable/level_map_volumn_seekbar_progress_unit" android:left="@dimen/volume_panel_icon_level1_left"/>
    <item android:gravity="center_vertical" android:height="@dimen/volume_panel_tick1_width" android:width="@dimen/volume_panel_tick_height" android:drawable="@drawable/level_map_volumn_seekbar_progress_unit" android:left="@dimen/volume_panel_icon_level2_left"/>
    <item android:gravity="center_vertical" android:height="@dimen/volume_panel_tick2_width" android:width="@dimen/volume_panel_tick_height" android:drawable="@drawable/level_map_volumn_seekbar_progress_unit" android:left="@dimen/volume_panel_icon_level3_left"/>
    <item android:gravity="center_vertical" android:height="@dimen/volume_panel_tick3_width" android:width="@dimen/volume_panel_tick_height" android:drawable="@drawable/level_map_volumn_seekbar_progress_unit" android:left="@dimen/volume_panel_icon_level4_left"/>
    <item android:gravity="center_vertical" android:height="@dimen/volume_panel_tick4_width" android:width="@dimen/volume_panel_tick_height" android:drawable="@drawable/level_map_volumn_seekbar_progress_unit" android:left="@dimen/volume_panel_icon_level5_left"/>
    <item android:gravity="center_vertical" android:id="@+id/progress_high" android:height="@dimen/volume_panel_icon_size" android:width="@dimen/volume_panel_icon_size" android:left="@dimen/volume_panel_icon_level6_left">
        <level-list>
            <item android:drawable="@drawable/vector_ic_mid_volume_high_26_blue" android:maxLevel="10000"/>
            <item android:drawable="@drawable/vector_ic_mid_volume_mute_26_grey" android:minLevel="10000" android:maxLevel="10001"/>
        </level-list>
    </item>
</layer-list>
