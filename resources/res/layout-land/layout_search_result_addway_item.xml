<?xml version="1.0" encoding="utf-8"?>
<com.xiaopeng.xui.widget.XRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/search_result_item_container" android:background="@drawable/drawable_ripple" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/search_result_scene_item_min_height">
    <com.xiaopeng.xui.widget.XTextView android:id="@+id/search_result_order" android:layout_marginTop="@dimen/search_result_scene_title_mt" android:layout_marginStart="@dimen/search_result_scene_order_ml" android:layout_marginEnd="@dimen/search_result_scene_order_mr" android:layout_alignParentStart="true" style="@style/search_order_style"/>
    <com.xiaopeng.montecarlo.views.SetWayPointView android:id="@+id/container_set_waypoint" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="24dp" android:layout_marginBottom="@dimen/search_result_scene_content_mb" android:layout_marginEnd="@dimen/destination_button_me" android:layout_alignParentEnd="true" app:interval="20dp" app:orientation="vertical"/>
    <com.xiaopeng.xui.widget.XLinearLayout android:orientation="vertical" android:id="@+id/search_result_item_detail" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_scene_title_mt" android:layout_marginBottom="@dimen/search_result_scene_content_mb" android:layout_marginEnd="@dimen/destination_button_ms" android:layout_toEndOf="@+id/search_result_order" app:vuiAction="@string/vui_action_click" app:vuiElementType="GROUP">
        <com.xiaopeng.xui.widget.XTextView android:id="@+id/search_result_name" android:drawablePadding="@dimen/search_result_name_ml" android:layout_marginEnd="@dimen/search_result_scene_address_me_one_button" style="@style/search_main_title_style"/>
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/search_result_address_container" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_address_type_mt" android:layout_marginEnd="@dimen/search_result_scene_address_me_one_button">
            <com.xiaopeng.xui.widget.XTextView android:textColor="@color/primary_text01" android:id="@+id/search_result_distance" style="@style/search_sub_title_style"/>
            <com.xiaopeng.xui.widget.XTextView android:textColor="@color/primary_text01" android:id="@+id/search_result_address_type" android:layout_marginEnd="@dimen/search_result_address_name_mr" style="@style/search_sub_title_style"/>
            <com.xiaopeng.xui.widget.XTextView android:id="@+id/search_result_address_name" style="@style/search_sub_title_style"/>
        </LinearLayout>
        <ViewStub android:id="@+id/rating_star_stub" android:layout="@layout/layout_rating_star" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt"/>
        <com.xiaopeng.xui.widget.XTextView android:id="@+id/search_result_open_time" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt" android:layout_marginEnd="@dimen/search_result_scene_address_me_one_button" style="@style/search_result_open_time_style"/>
        <ViewStub android:id="@+id/container_evaluation_stub" android:layout="@layout/layout_search_result_visit_list" android:inflatedId="@+id/container_evaluation" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ViewStub android:id="@+id/container_charge_info_stub" android:layout="@layout/layout_charge_status_for_search_list" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt" android:layout_marginEnd="10dp"/>
            <ViewStub android:id="@+id/container_park_info_stub" android:layout="@layout/layout_search_result_park_info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt" android:layout_marginEnd="10dp"/>
        </LinearLayout>
        <com.xiaopeng.xui.widget.XTextView android:id="@+id/tv_price_info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt" android:drawablePadding="@dimen/search_result_name_ml" android:layout_marginEnd="@dimen/search_result_scene_address_me_one_button" style="@style/search_result_price_info_style"/>
        <com.xiaopeng.xui.widget.XTextView android:id="@+id/tv_road_info" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt" style="@style/search_result_road_info_style"/>
        <ViewStub android:id="@+id/container_child_info_stub" android:layout="@layout/layout_search_result_addway_child_info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/search_result_container_child_mt"/>
