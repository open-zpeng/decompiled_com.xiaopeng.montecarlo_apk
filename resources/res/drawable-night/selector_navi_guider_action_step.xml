<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <gradient android:startColor="@color/gradient_end_13AC39" android:endColor="@color/gradient_start_69B616" android:angle="0" android:type="linear"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="oval">
            <gradient android:startColor="#2a3747" android:endColor="#5f6b80" android:angle="0" android:type="linear"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="oval">
            <size android:height="32dp" android:width="32dp"/>
            <solid android:color="@color/common_gray_10"/>
        </shape>
    </item>
</selector>
